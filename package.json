{
  "name": "docker-templates",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.5",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "pkg": "pkg dist/server.js --targets node12-win-x64",
    "package": "run-s \"build\" \"pkg\"",
    "docker": "docker-compose up -d",
    "compose": "run-s \"start\" \"docker\""
  },
  "dependencies": {
    "yaml": "^1.10.0"
  }
}
