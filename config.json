{
  "domain": "fredriklowenhamn.se",
  "volumes": "/mnt/filer/AppData",
  "data": "/mnt/filer",
  "ssl": true,
  "forwardAuth": "http://forward-auth:4181/",
  "apps": {
    "sonarr": {
      "image": "linuxserver/sonarr",
      "name": "sonarr",
      "configPath": "/config",
      "mountData": true,
      "services": [
        {
          "name": "sonarr",
          "port": 8989
        }
      ]
    },
    "radarr": {
      "image": "linuxserver/radarr",
      "name": "radarr",
      "configPath": "/config",
      "mountData": true,
      "services": [
        {
          "port": 7878
        }
      ]
    },
    "rethinkdb": {
      "name": "rethinkdb",
      "configPath": "/data",
      "services": 8080
    },
    "keycloak": {
      "name": "keycloak",
      "configPath": "/opt/jboss/keycloak/standalone/data/",
      "services": 8080,
      "environment": ["PROXY_ADDRESS_FORWARDING=true"]
    }
  }
}
