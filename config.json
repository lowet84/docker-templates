{
  "domain": "fredriklowenhamn.se",
  "volumes": "/mnt/filer/AppData",
  "data": "/mnt/filer",
  "ssl": true,
  "forwardAuth": "http://forward-auth:4181/",
  "vpn": true,
  "apps": {
    "sonarr": {
      "image": "ghcr.io/linuxserver/sonarr",
      "configPath": "/config",
      "dataPath": "/data",
      "services": 8989
    },
    "radarr": {
      "image": "ghcr.io/linuxserver/radarr",
      "configPath": "/config",
      "dataPath": "/data",
      "services": 7878
    },
    "rethinkdb": {
      "configPath": "/data",
      "services": 8080
    },
    "keycloak": {
      "image": "jboss/keycloak",
      "configPath": "/opt/jboss/keycloak/standalone/data/",
      "services": [{ "port": 8080, "insecure": true }],
      "environment": ["PROXY_ADDRESS_FORWARDING=true"]
    },
    "nexus": {
      "image": "sonatype/nexus3",
      "configPath": "/nexus-data",
      "services": [
        {
          "name": "nexus",
          "port": 8081
        },
        {
          "name": "docker",
          "port": 9000,
          "insecure": true
        }
      ]
    },
    "forward-auth": {
      "image": "docker.fredriklowenhamn.se/traefik-forward-auth",
      "services": 4181,
      "environment": [
        "CLIENT_ID=#AUTH_CLIENT_ID#",
        "CLIENT_SECRET=#AUTH_CLIENT_SECRET#",
        "PROVIDER_URI=#AUTH_PROVIDER_URI#",
        "SECRET=#AUTH_SECRET#",
        "WHITELIST=#EMAIL#"
      ]
    },
    "wol": {
      "image": "dabondi/go-rest-wol",
      "services": 8080,
      "configPath": "/app/config",
      "environment": ["WOLFILE=./config/computer.csv"]
    },
    "youtransfer": {
      "image": "remie/youtransfer",
      "services": 5000,
      "configPath": {
        "config": "/opt/youtransfer/config",
        "uploads": "/opt/youtransfer/uploads"
      }
    },
    "transmission": {
      "image": "ghcr.io/linuxserver/transmission",
      "services": [
        {
          "port": 9091,
          "insecure": true
        }
      ],
      "configPath": "/config",
      "dataPath": "/data",
      "vpn": true,
      "environment": [
        "USER=#USERNAME#",
        "PASS=#PASSWORD#"
      ]
    },
    "plex": {
      "image": "ghcr.io/linuxserver/plex",
      "net": "host",
      "configPath": "/config",
      "dataPath": "/data"
    },
    "guacamole": {
      "image": "oznu/guacamole",
      "configPath": "/config",
      "services": 8080
    },
    "kong-db": {
      "image": "cassandra:3",
      "configPath": "/var/lib/cassandra"
    },
    "kong": {
      "image": "kong:alpine",
      "environment": [
        "KONG_DATABASE=cassandra",
        "KONG_CASSANDRA_CONTACT_POINTS=kong-db",
        "KONG_ADMIN_LISTEN=0.0.0.0:8001, 0.0.0.0:8444 ssl"
      ],
      "services": [{ "port": 8000, "insecure": true }]
    },
    "konga": {
      "image": "pantsel/konga",
      "services": [{ "port": 1337, "insecure": true }],
      "configPath": "/app/kongadata",
      "environment":[
        "TOKEN_SECRET=sdlkifujhu4k3uh5i3u4hb5ku34hb5k345"
      ]
    },
    "demo": {
      "image": "nginxdemos/hello"
    },
    "bbk": {
      "image": "bbk",
      "configPath": "/app/log",
      "services": 5700
    }
  }
}
